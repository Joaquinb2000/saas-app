<%= link_to tenant_project_path(@tenant.id,project.id) do %>
    <div class="col-sm-6 col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3><%= project.title %></h3>
            </div>
            <div class="panel-body">
                <p> <%= project.details.truncate( 30 , separator:' ')%> </p>
            </div>
<% end %>
            <div class="panel-footer">
                <%= link_to "Edit users in project", users_tenant_project_path(tenant_id: project.tenant_id, id: project),
                class: "btn btn-info" %>
            </div>
        </div>
    </div>
